{
  "version": 2,
  "builds": [
    {
      "src": "./realgram-backend/src/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "./realgram-frontend/dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "./realgram-backend/src/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "./realgram-frontend/dist/$1"
    }
  ]
}
