{
  "version": 2,
  "builds": [
    {
      "src": "./realgram-frontend/dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*.(js|css|png|jpg|jpeg|gif|svg|json|woff|woff2))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "./realgram-frontend/dist/$1"
    }
  ]
}
